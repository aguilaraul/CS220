/**
 *  Exclusive-or gate:
 *  out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
	Nand(a=a, b=b, out=outab);
	Nand(a=a, b=outab, out=or1);
	Nand(a=outab, b=b, out=or2);
	Nand(a=or1, b=or2, out=out);
}